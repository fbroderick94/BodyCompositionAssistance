<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fyp.nuigalway.ie.bodycompostionassistance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="fyp.nuigalway.ie.bodycompostionassistance.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="fyp.nuigalway.ie.bodycompostionassistance.EditActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name="fyp.nuigalway.ie.bodycompostionassistance.SelectMealActivity"></activity>

        <provider
            android:name="fyp.nuigalway.ie.bodycompostionassistance.data.FoodProvider"
            android:authorities="fyp.nuigalway.ie.bodycompostionassistance"
            android:exported="false" />
    </application>

</manifest>